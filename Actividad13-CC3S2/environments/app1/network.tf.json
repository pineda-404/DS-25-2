
{
    "variable": [
        {
            "name": [
                {
                    "type": "string",
                    "default": "new-hello-world",
                    "description": "Nuevo nombre del servidor local"
                }
            ]
        },
        {
            "network": [
                {
                    "type": "string",
                    "default": "lab-net",
                    "description": "Nombre de la red local"
                }
            ]
        },
        {
            "port": [
                {
                    "type": "number",
                    "default": 8080,
                    "description": "Puerto del servidor"
                }
            ]
        },
        {
            "api_key": [
                {
                    "type": "string",
                    "sensitive": true,
                    "description": "API Key para el servidor"
                }
            ]
        }
    ]
}
